{
    "env": {
        "browser": true,
            "node": true
    },
    "plugins": [
        "react"
    ],
    "ecmaFeatures": {
        "arrowFunctions": true,
        "blockBindings": true,
        "classes": true,
        "destructuring": true,
        "forOf": true,
        "generators": true,
        "objectLiteralComputedProperties": true,
        "objectLiteralShorthandMethods": true,
        "objectLiteralShorthandProperties": true,
        "restParams": true,
        "spread": true,
        "templateStrings": true,
        "jsx": true
    },
    "rules": {
        "comma-dangle": 1,
        "no-cond-assign": 1,
        "no-console": 1,
        "no-constant-condition": 1,
        "no-control-regex": 1,
        "no-debugger": 1,
        "no-dupe-args": 1,
        "no-dupe-keys": 1,
        "no-duplicate-case": 1,
        "no-empty-character-class": 1,
        "no-empty": 1,
        "no-ex-assign": 1,
        "no-extra-boolean-cast": 1,
        "no-extra-parens": 1,
        "no-extra-semi": 1,
        "no-func-assign": 1,
        "no-inner-declarations": 1,
        "no-invalid-regexp": 1,
        "no-irregular-whitespace": 1,
        "no-negated-in-lhs": 1,
        "no-obj-calls": 1,
        "no-regex-spaces": 1,
        "no-sparse-arrays": 1,
        "no-unexpected-multiline": 1,
        "no-unreachable": 1,
        "use-isnan": 1,
        "valid-typeof": 1,
        "accessor-pairs": 1,
        "block-scoped-var": 1,
        "consistent-return": 1,
        "curly": 1,
        "dot-location": [1, "property"],
        "dot-notation": 1,
        "eqeqeq": [1, "allow-null" ],
        "no-caller": 1,
        "no-case-declarations": 1,
        "no-div-regex": 1,
        "no-empty-pattern": 1,
        "no-eval": 1,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-fallthrough": 1,
        "no-floating-decimal": 1,
        "no-implied-eval": 1,
        "no-invalid-this": 1,
        "no-iterator": 1,
        "no-labels": 1,
        "no-lone-blocks": 1,
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-native-reassign": 1,
        "no-new-func": 1,
        "no-new-wrappers": 1,
        "no-new": 1,
        "no-octal-escape": 1,
        "no-octal": 1,
        "no-process-env": 1,
        "no-proto": 1,
        "no-redeclare": 1,
        "no-return-assign": 1,
        "no-script-url": 1,
        "no-self-compare": 1,
        "no-sequences": 1,
        "no-throw-literal": 1,
        "no-unused-expressions": [1, {
            "allowShortCircuit": true,
            "allowTernary": true
        }],
        "no-useless-call": 1,
        "no-useless-concat": 1,
        "no-void": 1,
        "no-warning-comments": [1, {
            "terms": [
                "todo",
            "tbd"
                ],
            "location": "start"
        }],
        "no-with": 1,
        "radix": 1,
        "strict": [1, "global"],
        "no-catch-shadow": 1,
        "no-delete-var": 1,
        "no-label-var": 1,
        "no-shadow-restricted-names": 1,
        "no-undef-init": 1,
        "no-undef": 1,
        "no-unused-vars": 1,
        "no-use-before-define": [1, "nofunc"],
        "callback-return": 1,
        "global-require": 1,
        "handle-callback-err": 1,
        "no-mixed-requires": 1,
        "no-new-require": 1,
        "no-path-concat": 1,
        "no-process-exit": 1,
        "array-bracket-spacing": 1,
        "block-spacing": 1,
        "brace-style": 1,
        "camelcase": 1,
        "comma-spacing": 1,
        "comma-style": 1,
        "computed-property-spacing": 1,
        "eol-last": 1,
        "indent": [1, 2, {
            "SwitchCase": 1
        }],
        "jsx-quotes": 1,
        "key-spacing": 1,
        "linebreak-style": 0,
        "lines-around-comment": 1,
        "max-depth": 1,
        "max-len": [1, 120],
        "new-cap": 1,
        "new-parens": 1,
        "newline-after-var": 0,
        "no-array-constructor": 1,
        "no-bitwise": 1,
        "no-inline-comments": 1,
        "no-lonely-if": 1,
        "no-mixed-spaces-and-tabs": 1,
        "no-multiple-empty-lines": 1,
        "no-negated-condition": 1,
        "no-nested-ternary": 0,
        "no-new-object": 1,
        "no-spaced-func": 1,
        "no-trailing-spaces": 1,
        "no-unneeded-ternary": 1,
        "object-curly-spacing": 1,
        "one-var": [1, "never"],
        "operator-assignment": 1,
        "operator-linebreak": [1, "before"],
        "padded-blocks": [1, "never"],
        "quote-props": [1, "consistent-as-needed", {
            "keywords": true
        }],
        "quotes": [1, "single"],
        "require-jsdoc": [1, {
            "require": {
                "FunctionDeclaration": false,
                "MethodDefinition": false,
                "ClassDeclaration": false
            }
        }],
        "semi-spacing": 1,
        "semi": 1,
        "space-after-keywords": 1,
        "space-before-blocks": 1,
        "space-before-function-paren": [1, {
            "anonymous": "always",
            "named": "never"
        }],
        "space-before-keywords": 1,
        "space-in-parens": 1,
        "space-infix-ops": 1,
        "space-return-throw-case": 1,
        "space-unary-ops": 1,
        "spaced-comment": 1,
        "arrow-parens": 1,
        "arrow-spacing": 1,
        "constructor-super": 1,
        "generator-star-spacing": [1, {
            "after": false
        }],
        "no-arrow-condition": 1,
        "no-class-assign": 1,
        "no-const-assign": 1,
        "no-dupe-class-members": 1,
        "no-this-before-super": 1,
        "no-var": 1,
        "object-shorthand": 0,
        "prefer-arrow-callback": 0,
        "prefer-const": 0,
        "prefer-reflect": 0,
        "prefer-spread": 1,
        "prefer-template": 0,
        "require-yield": 1
    }
}
